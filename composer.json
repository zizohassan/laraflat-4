{
    "name": "laraflat/laraflat",
    "description": "laraflat package",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Abdel Aziz hassan",
            "email": "zizo1999988@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravelcollective/html":"^5.4.0",
        "mcamara/laravel-localization": "^1.3",
        "chumper/zipper": "1.0.x"
    },
    "autoload": {
        "psr-4": {
            "Laraflat\\Laraflat\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "orchestra/testbench-browser-kit": "~3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflat\\Laraflat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflat\\Laraflat\\Providers\\LaraflatServiceProvider"
            ]
        }
    }
}
